
<%- include("partials/header"); -%>
<%- include('partials/adminNavbar') %>

<div class="container">
    <h1>Compose</h1>
<form class="" action="/editBlog" method="post">
  <div class="form-group">
    <input class="form-control" type="text" name="blogId" value = "<%= editpost.id %> " hidden>
    <label>Title</label>
    <input class="form-control" type="text" name="title" value = "<%= editpost.title %>">
    <label>Post</label>
    <textarea class="form-control" name="post" rows="5" cols="30" ><%= editpost.post %></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Save</button>
</form>

</div>

</body>
</html>
